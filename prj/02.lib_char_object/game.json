{
	"width": 320,
	"height": 512,
	"fps": 30,
	"main": "./script/src/main.js",
	"assets": {
		"player": {
			"type": "image",
			"width": 32,
			"height": 32,
			"path": "image/player.png"
		},
		"shot": {
			"type": "image",
			"width": 16,
			"height": 16,
			"path": "image/shot.png"
		},
		"se": {
			"type": "audio",
			"path": "audio/se",
			"systemId": "sound",
			"duration": 250,
			"hint": {
				"extensions": [
					".aac",
					".ogg"
				]
			}
		},
		"CharEntity": {
			"type": "script",
			"path": "script/lib/CharEntity.js",
			"global": true
		},
		"CharFactory": {
			"type": "script",
			"path": "script/lib/CharFactory.js",
			"global": true
		},
		"main": {
			"type": "script",
			"path": "script/src/main.js",
			"global": true
		}
	},
	"environment": {
		"sandbox-runtime": "3"
	},
	"globalScripts": []
}